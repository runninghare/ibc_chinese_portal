        "lint": "ionic-app-scripts lint",
        "ionic:build": "ionic-app-scripts build",
        "ionic:serve": "ionic-app-scripts serve",
        "ionic:sign": "pushd platforms/android && rm -rf app/build/outputs/apk/release/android-release*.apk && gradlew assembleRelease && cd app/build/outputs/apk/release && zipalign -v -p 4 app-release-unsigned.apk android-release-aligned.apk && apksigner sign --ks ~/android/android_keys/ibc-release.keystore --ks-key-alias ibc_chinese --out android-release-signed.apk android-release-aligned.apk && popd"
