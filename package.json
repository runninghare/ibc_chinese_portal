        "emulate-dev": "IBC_ENV=ibc_dev ionic cordova emulate ios",
        "emulate-ipad": "IBC_ENV=ibc_prod ionic cordova emulate ios --target='iPad-Pro--12-9-inch---2nd-generation-'",
